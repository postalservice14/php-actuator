{
    "name": ":postalservice14/php-actuator",
    "type": "library",
    "description": "PHP port of Spring Actuator which provides an easy health checking capability of services.",
    "keywords": [
        "actuator",
        "health",
        "healthcheck"
    ],
    "homepage": "https://github.com/postalservice14/php-actuator",
    "license": "MIT",
    "authors": [
        {
            "name": "John Kelly",
            "email": "johnmkelly86@gmail.com",
            "homepage": "http://www.github.com/postalservice14",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "silex/silex": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Actuator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actuator\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
